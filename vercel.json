{
  "functions": {
    "apps/api/src/main.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "outputDirectory": "apps/client/dist",
  "routes": [
    {
      "src": "/",
      "dest": "apps/client/dist"
    },
    {
      "src": "/.*",
      "dest": "apps/api/src/main.ts"
    }
  ]
}
